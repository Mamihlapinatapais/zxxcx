<view class="superbox1">
    <view class="box1">

    </view>
    <view class="pass">
        <image src="../../../images/pass.png" />
    </view>
    <view class="txt1">
        面试通过
    </view>
    <view class="box2">

    </view>
    <view class="avatar">
        <image src="../../../images/ceshi/2.jpg" />
    </view>
    <view class="txt2">
        恭喜{{name}}同学通过云顶书院的面试
    </view>
</view>
<view class="share">
    <button class="button" hover-class="button-hover" bindtap = "result">生成分享图片</button>
</view>
<view class="click_copy">
    <view class="leftinfo">
        <view class="txt3">
            面试通过的同学加
        </view>
        <view class="txt4">
            <text decode="{{true}}">qq群&nbsp;:&nbsp;</text>
        </view>
        <view class="group_number">
            <text decode="{{true}}">{{group_number}}&nbsp;&nbsp;&nbsp;&nbsp;</text>
        </view>
        <view class="txt5">
            请勿外传群号
        </view>
    </view>
    <view class="copy" bindtap = "copy">
        点此复制群号
    </view>

</view>