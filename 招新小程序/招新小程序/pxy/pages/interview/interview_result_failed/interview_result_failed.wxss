.superbox1{
    margin: 20rpx;
    border-radius: 10rpx;
    width: 710rpx;
    height: 600rpx;
    background-color: #fff;
}
.superbox1 .box1{
    height: 360rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    align-items: center;
}
.superbox1 .box1 image{
    width: 144rpx;
    height: 192rpx;
}
.superbox1 .box2{
    display: flex;
    flex-direction: row;
    justify-content: center;
    font-size: 30rpx;
    color: #7E8388;
    margin-top: 30rpx;
}